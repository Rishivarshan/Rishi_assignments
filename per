def factorial(n):
	return 1 if (n==1 or n==0) else n * factorial(n - 1)
def fibonacci(n):
	a = 0
	b = 1
	if n < 0:
		print("Incorrect input")
	elif n == 0:
		return a
	elif n == 1:
		return b
	else:
		for i in range(2, n+1):
			c = a + b
			a = b
			b = c
		return b
def isPalindrome(s):
	return s == s[::-1]

ans = isPalindrome(s)
